</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NamaPelajaran)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NamaPelajaran)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdMapel }) |
